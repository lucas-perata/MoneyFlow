{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "MoneyFlow",
  "version": "0.1.0",
  "identifier": "com.moneyflow.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "MoneyFlow",
        "width": 550,
        "height": 750,
        "maximizable": false,
        "alwaysOnTop": false,
        "visible":true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png"
    ]
  }
}
